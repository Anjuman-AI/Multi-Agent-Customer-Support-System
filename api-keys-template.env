# AI Customer Support System - Environment Variables Template
# Copy this file to .env and replace all ********** with your actual credentials
# NEVER commit the actual .env file to version control!

# ============================================================================
# OPENAI CONFIGURATION
# ============================================================================
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-**********************************************

# Model configuration (optional - defaults are set in workflows)
OPENAI_MODEL=gpt-4
OPENAI_TEMPERATURE_TRIAGE=0.3
OPENAI_TEMPERATURE_BUG=0.7
OPENAI_TEMPERATURE_BILLING=0.6
OPENAI_TEMPERATURE_FEATURE=0.8
OPENAI_TEMPERATURE_GENERAL=0.7
OPENAI_TEMPERATURE_ESCALATION=0.2

# Token limits
OPENAI_MAX_TOKENS_TRIAGE=300
OPENAI_MAX_TOKENS_SPECIALIST=500
OPENAI_MAX_TOKENS_ESCALATION=200

# ============================================================================
# AIRTABLE CONFIGURATION
# ============================================================================
